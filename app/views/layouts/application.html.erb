<!DOCTYPE html>
<html<%= content_for(:angular_app) %>>
  <head>
    <title>AnglifiedBootstrap</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>

    <!-- For Community Page -->
    <!-- font awesome -->
    <link
    href="https://maxcdn.bootstrapcdn.com/font-awesome/4.6.3/css/font-awesome.min.css"
    rel="stylesheet"
    integrity="sha384-T8Gy5hrqNKT+hzMclPo118YTQO6cYprQmhrYwIiQ/3axmI1hQomh7Ud2hPOy8SP1"
    crossorigin="anonymous">

    <!-- Fonts -->
    <link href='http://fonts.googleapis.com/css?family=Open+Sans' rel='stylesheet'  type='text/css'>

    <!-- Bstrp4 -->
    <link <%= content_for(:bs4_community) %>>

    <!--md bootstrap -->
    <link <%= content_for(:mdb_community) %>>

    <!-- For Templates Edit Page -->
    <!-- Custom Fonts -->
    <link <%= content_for(:lato_font) %>>
    <link <%= content_for(:catamaran_font) %>>
    <link <%= content_for(:muli_font) %>>

    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
        <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->

    <!-- Plugin JavaScript -->
    <script <%= content_for(:plugin_js) %>></script>


  </head>

  <body <%= content_for(:body_id) %>>
    <%= yield %>
  </body>
</html>
